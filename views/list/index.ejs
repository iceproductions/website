<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Aztec server list</title>
</head>
<body>
    Public guilds:
    <table>
        <tr>
            <th>Name</th>
            <th>Member count</th>
            <th>Votes</th>
        </tr>
        <% for(var guild of guilds) { %>
            <tr>
                <td><a href="/list/<%=guild.id %>"><%=guild.name %></a></td>
                <td><%=guild.memberCount %></td>
                <td><%=guild.votes %></td>
            </tr>
        <% } %>
    </table>
    <% if(!guilds) var guilds = []; %>
    <% if(!guilds.length) { %>
        Guild list isn't available
    <% } %>
</body>
</html>